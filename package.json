{
  "name": "gql-workshop",
  "version": "0.0.0",
  "main": "index.js",
  "description": "GraphQL workshop",
  "repository": {
    "type": "git",
    "url": "https://github.com/leifoolsen/gql-workshop.git"
  },
  "license": "MIT",
  "keywords": [
    "boilerplate",
    "graphql"
  ],
  "engines": {
    "node": ">=8.9.0"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11"
  ],
  "jest": {
    "notify": true,
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/tools/jest/setup.js"
    ],
    "globals": {
      "__DEV__": true
    },
    "moduleDirectories": [
      "node_modules",
      "src/client"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "clean": "rimraf build dist coverage",
    "setup": "npm run clean && npm install",
    "start": "cross-env NODE_ENV=production node ./dist/bin/index.js",
    "start:dev": "cross-env NODE_ENV=development node ./src/bin/index.js",
    "prestart:api": "rimraf build",
    "start:api": "cross-env NODE_ENV=development webpack --config ./tools/webpack/webpack.config.server.babel.js",
    "prebuild": "npm run clean",
    "build": "npm-run-all build:*",
    "build:client": "cross-env NODE_ENV=production webpack -p --progress --display-error-details --config ./tools/webpack/webpack.config.babel.js",
    "build:server": "cross-env NODE_ENV=production webpack --progress --config ./tools/webpack/webpack.config.server.babel.js",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint --ignore-path .gitignore ./src/**/*.js --max-warnings 10",
    "lint:style": "echo Stylelint not implemented",
    "test": "npm-run-all test:*",
    "test:unit": "cross-env NODE_ENV=test jest --config=tools/jest/jest.unit.config.js --coverage --forceExit",
    "test:it": "cross-env NODE_ENV=test jest --config=tools/jest/jest.integration.config.js --coverage --forceExit",
    "release:patch": "npm run lint && npx standard-version -r patch && git push --follow-tags origin master",
    "release:minor": "npm run lint && npx standard-version -r minor && git push --follow-tags origin master",
    "release:major": "npm run lint && npx standard-version -r major && git push --follow-tags origin master"
  },
  "postcss": {},
  "devDependencies": {
    "apollo-server-express": "1.3.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.1",
    "babel-loader": "7.1.4",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-regenerator": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "babel-watch": "2.0.7",
    "body-parser": "1.18.2",
    "browserslist": "3.1.2",
    "chalk": "2.3.2",
    "compression": "1.7.2",
    "connect-history-api-fallback": "1.5.0",
    "copy-webpack-plugin": "4.5.1",
    "copyfiles": "2.0.0",
    "core-js": "2.5.3",
    "cors": "2.8.4",
    "cross-env": "5.1.4",
    "css-loader": "0.28.10",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-webpack": "0.8.4",
    "eslint-loader": "2.0.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-compat": "2.2.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "express": "4.16.3",
    "extract-text-webpack-plugin": "3.0.2",
    "graphql": "0.13.1",
    "graphql-tools": "2.22.0",
    "helmet": "3.12.0",
    "hpp": "0.2.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.0.6",
    "http-proxy-middleware": "0.18.0",
    "husky": "0.15.0-rc.9",
    "identity-obj-proxy": "3.0.0",
    "import-fresh": "2.0.0",
    "jest": "22.4.2",
    "nconf": "0.10.0",
    "node-sass": "4.7.2",
    "npm-run-all": "4.1.2",
    "postcss-load-config": "1.2.0",
    "postcss-loader": "2.1.1",
    "react-hot-loader": "4.0.0",
    "react-test-renderer": "16.2.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.7",
    "source-map-support": "0.5.3",
    "start-server-webpack-plugin": "2.2.5",
    "style-loader": "0.20.3",
    "webpack": "3.11.0",
    "webpack-dev-middleware": "2.0.6",
    "webpack-hot-middleware": "2.21.2",
    "webpack-node-externals": "1.6.0",
    "winston": "2.4.1"
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.1.11",
    "apollo-client": "2.2.7",
    "apollo-link-http": "1.5.3",
    "axios": "0.18.0",
    "babel-polyfill": "6.26.0",
    "base-64": "0.1.0",
    "closest": "0.0.1",
    "core-js": "2.5.3",
    "graphql-tag": "2.8.0",
    "harmony-reflect": "1.6.0",
    "intl": "1.2.5",
    "lodash": "4.17.5",
    "prop-types": "15.6.1",
    "raf": "3.4.0",
    "react": "16.2.0",
    "react-apollo": "2.0.4",
    "react-dom": "16.2.0",
    "react-intl": "2.4.0",
    "react-router-dom": "4.2.2",
    "uuid": "3.2.1"
  }
}
